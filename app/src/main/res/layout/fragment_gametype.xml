<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:fancy="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.shixels.thankgodrichard.enellottoapp.Games.Gametype">

    <!-- TODO: Update blank fragment layout -->
 <LinearLayout
     android:layout_width="match_parent"
     android:orientation="vertical"
     android:layout_height="match_parent">
     <FrameLayout
         android:layout_width="match_parent"
         android:layout_height="0dp"
         android:layout_weight="1">
         <ImageView
             android:layout_width="match_parent"
             android:src="@drawable/threestar_gaetypr"
             android:id="@+id/threeStarImg"
             android:scaleType="fitXY"
             android:layout_marginTop="20dp"
             android:layout_marginLeft="@dimen/activity_horizontal_margin"
             android:layout_marginRight="@dimen/activity_horizontal_margin"
             android:layout_height="fill_parent" />
         <RelativeLayout
             android:layout_width="match_parent"
             android:id="@+id/threestar"
             android:clickable="true"
             android:layout_height="match_parent">
             <TextView
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 android:text="TesingView"
                 android:id="@+id/threestarnextdraw"
                 android:textColor="@color/textColorGreen"
                 android:layout_alignParentBottom="true"
                 android:layout_centerHorizontal="true"
                 android:layout_marginBottom="20dp"/>
         </RelativeLayout>

     </FrameLayout>
     <FrameLayout
         android:layout_width="match_parent"
         android:layout_height="00dp"
         android:layout_weight="1">

         <ImageView
             android:layout_width="match_parent"
             android:src="@drawable/fivestar_gametypr"
             android:layout_marginRight="@dimen/activity_horizontal_margin"
             android:layout_marginLeft="@dimen/activity_horizontal_margin"
             android:layout_height="match_parent"
             android:scaleType="fitXY"
             android:id="@+id/imageView2" />
         <RelativeLayout
             android:layout_width="match_parent"
             android:id="@+id/fivestar"
             android:clickable="true"
             android:layout_height="match_parent">
             <TextView
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 android:text="TesingView"
                 android:id="@+id/fivestarnextdraw"
                 android:textColor="@color/textColorGreen"
                 android:layout_alignParentBottom="true"
                 android:layout_centerHorizontal="true"
                 android:layout_marginBottom="20dp"/>

         </RelativeLayout>


     </FrameLayout>
     <FrameLayout
         android:layout_width="match_parent"
         android:layout_height="0dp"
         android:layout_weight="1">
         <ImageView
             android:layout_width="match_parent"
             android:src="@drawable/sixstar_gametype"
             android:scaleType="fitXY"
             android:layout_marginBottom="20dp"
             android:layout_marginLeft="@dimen/activity_horizontal_margin"
             android:layout_marginRight="@dimen/activity_horizontal_margin"
             android:layout_height="match_parent" />
         <RelativeLayout
             android:layout_width="match_parent"
             android:id="@+id/sixstar"
             android:clickable="true"
             android:layout_height="match_parent">
             <TextView
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 android:text="TesingView"
                 android:id="@+id/sixstrrnextDraw"
                 android:textColor="@color/textColorGreen"
                 android:layout_alignParentBottom="true"
                 android:layout_centerHorizontal="true"
                 android:layout_marginBottom="35dp"/>

         </RelativeLayout>

     </FrameLayout>

 </LinearLayout>

</FrameLayout>
